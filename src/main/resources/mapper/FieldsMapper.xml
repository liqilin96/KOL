<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.weihu.kol.db.dao.FieldsDao">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.weihu.kol.db.po.Fields">
        <id column="id" property="id"/>
        <result column="name" property="name"/>
        <result column="field_list" property="fieldList"/>
        <result column="ctime" property="ctime"/>
        <result column="utime" property="utime"/>
        <result column="create_user_id" property="createUserId"/>
        <result column="update_user_id" property="updateUserId"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id
        , name, field_list, ctime, utime, create_user_id, update_user_id
    </sql>

</mapper>
